    private void txtCI_RucActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        if( txtCI_Ruc.getText().length() == 10 ){
            buscarPaciente();
            cargarOdontograma();            
        }else{
            JOptionPane.showMessageDialog(null,"Error en el n√∫mero de CI: " 
                            + txtCI_Ruc.getText(),"Mensaje del sistema", JOptionPane.WARNING_MESSAGE);
            limpiarCampos();
        }
    }                                         

    private void txtCI_RucKeyTyped(java.awt.event.KeyEvent evt) {                                   
        // TODO add your handling code here:
        numeroCaracteres(evt, 10, evt.getSource());
        soloNumeros(evt);
    }                                  

    private void btnCancelarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
//        Principal_menu obj = new Principal_menu();
//        obj.setVisible(true);
        cerrarConexion();
        dispose();
    }                                           

    //***************************************//
    //***************************************//
    //*********  F U N C I O N E S  *********//  
    //***************************************//
    //***************************************//
    
    private void numeroCaracteres( java.awt.event.KeyEvent evt,  int max, Object obj){
        JTextField txt = (JTextField)obj;        
        if ( txt.getText().length() > max ){
            evt.consume();
            JOptionPane.showMessageDialog(rootPane, "El n√∫mero m√°ximo de caracteres es "+max);
            txt.requestFocus();
        }   
    }
    
    private void soloNumeros( java.awt.event.KeyEvent evt ){
        char c = evt.getKeyChar(); 
        if(Character.isLetter(c)) { 
              getToolkit().beep(); 
              evt.consume();
              JOptionPane.showMessageDialog(null,"Solo se admite numeros" ,
                      "Validaci√≥n", JOptionPane.ERROR_MESSAGE);
          }
    }  
    
    private void limpiarCampos(){
        txtCI_Ruc.setText("");
        txtNombre.setText("");
        txtApellido.setText("");
        txtNacimiento.setText("");
        txtDireccion.setText("");
        txtCelular.setText("");
        txtCorreo.setText("");
        txtPiezas.setText("");
        txtFecha.setText("");
        hombre.setSelected(false);
        mujer.setSelected(false); 
    }
    
    public void cargarOdontograma(){
        Statement st;
        ResultSet rs;
        
        String [] titulos = {"No.Piezas", "Detalle", "Mesial", "Distal", "Octusal", "Palatino", "Lingual", "Vestibular"};
        String [] registros = new String[8];
        String sql1 = "call InformeOdontogramaProcedure('" + txtCI_Ruc.getText() + "')";
        modelo = new DefaultTableModel(null, titulos);
        
        try {
            st = cn.createStatement();
            rs = st.executeQuery(sql1);
                        
            while ( rs.next() ) {
                registros[0]= rs.getString("Numero");
                registros[1]= rs.getString("Detalle");
                registros[2]= rs.getString("Mesial");
                registros[3]= rs.getString("Distal");
                registros[4]= rs.getString("Octusal");
                registros[5]= rs.getString("Palatino");
                registros[6]= rs.getString("Lingual");
                registros[7]= rs.getString("Vestibular");
                modelo.addRow(registros);
                tblPiezas.setModel(modelo);                
            }
        } catch (SQLException sqle) {
            JOptionPane.showMessageDialog(null, "C√≥digo de Error: " + sqle.getErrorCode() + "\n" +
                    "Estado de Error: " + sqle.getSQLState() + "\n" + 
                    "Mensaje de Error: " + sqle.getMessage(),
                    "Advertencia", JOptionPane.ERROR_MESSAGE);
        }
    }
    
    public void buscarPaciente(){
        Statement st;
        ResultSet rs;
        String [] registros = new String[10];
        String procedure = "{call buscarOdontPac('" + txtCI_Ruc.getText() + "')}";
        limpiarCampos();
            try {                           
                st = cn.createStatement();
                rs = st.executeQuery(procedure);
                                  
                while ( rs.next() ){
                    registros[0]= rs.getString("CI");
                    registros[1]= rs.getString("Nombre");
                    registros[2]= rs.getString("Apellido");
                    registros[3]= rs.getString("Sexo");
                    registros[4]= rs.getString("FNacimiento");
                    registros[5]= rs.getString("Celular");
                    registros[6]= rs.getString("Direccion");
                    registros[7]= rs.getString("Email");
                    registros[8]= rs.getString("Piezas");
                    registros[9]= rs.getString("Fecha");
                }
                if( registros[0] == null ){
                    JOptionPane.showMessageDialog(null,"No se ha encontrado ningun Paciente con CI: " 
                            + txtCI_Ruc.getText(),"Mensaje del sistema", JOptionPane.WARNING_MESSAGE);
                    limpiarCampos();
                } else {   
                    txtCI_Ruc.setText(registros[0]);
                    txtNombre.setText(registros[1]);
                    txtApellido.setText(registros[2]);
                    txtNacimiento.setText(registros[4]);
                    txtCelular.setText(registros[5]);
                    txtDireccion.setText(registros[6]);
                    txtCorreo.setText(registros[7]);
                    txtPiezas.setText(registros[8]);
                    txtFecha.setText(registros[9]);
                    if( registros[3].equals("M") )
                        hombre.setSelected(true);
                    else
                        mujer.setSelected(true);
                }
                
            }catch (SQLException sqle){
                JOptionPane.showMessageDialog(null, "C√≥digo de Error: " + sqle.getErrorCode() + "\n" +
                        "Estado de Error: " + sqle.getSQLState() + "\n" + 
                        "Mensaje de Error: " + sqle.getMessage(),
                        "Advertencia", JOptionPane.ERROR_MESSAGE);
            }
    }
    
    public void cerrarConexion(){
        try {
            cn.close();
            System.out.println("Desconectado: " + cn.isClosed());  
        } catch (SQLException sqle) {
            JOptionPane.showMessageDialog(null, "C√≥digo de Error: " + sqle.getErrorCode() + "\n" +
                        "Estado de Error: " + sqle.getSQLState() + "\n" + 
                        "Mensaje de Error: " + sqle.getMessage(),
                        "Advertencia", JOptionPane.ERROR_MESSAGE);}
    }
    
