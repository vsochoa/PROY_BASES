    private void btnGuardarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        if( txtFecha.getText().equals("") || txtSecuencia.getText().equals("") ||
            txtNombre.getText().equals("") || txtCedula_Ruc.getText().equals("") ||
            txtTelefono.getText().equals("") || txtDireccion.getText().equals("") ||
            txtCantidad.getText().equals("") || txtDetalle.getText().equals("") ||
            txtValor.getText().equals("") || txtSubtotal.getText().equals("") ||
            txtDescuencto.getText().equals("") || txtIva.getText().equals("") ||
            txtTotal.getText().equals("") ){
            JOptionPane.showMessageDialog(null, "Faltan datos por ingresar.",
                "Error", JOptionPane.ERROR_MESSAGE);
            txtCantidad.requestFocus();
        }else{
            JOptionPane.showMessageDialog(null, "Se ha registrado con exito.",
                "Confirmación", JOptionPane.INFORMATION_MESSAGE);
            limpiarCampos();
        }
    }                                          

    private void btnLimpiarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        limpiarCampos();
    }                                          

    private void btnCancelarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        Principal_menu obj = new Principal_menu();
        dispose();
        obj.setVisible(true);
    }                                           

    private void txtFechaActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
    }                                        

    //***************************************//
    //***************************************//
    //*********  F U N C I O N E S  *********//  
    //***************************************//
    //***************************************//
    
    private void numeroCaracteres( java.awt.event.KeyEvent evt,  int maximo, Object obj){
        JTextField txt = (JTextField)obj;
        
        if ( txt.getText().length() >= maximo ){
            evt.consume();
            JOptionPane.showMessageDialog(rootPane, "El número máximo de caracteres es "+maximo);
            txt.requestFocus();
        }   
    }
    
    private void limpiarCampos(){
        txtFecha.setText("");
        txtSecuencia.setText("");
        txtNombre.setText("");
        txtCedula_Ruc.setText("");
        txtTelefono.setText("");
        txtDireccion.setText("");
        txtCantidad.setText("");
        txtDetalle.setText("");
        txtValor.setText("");
        txtSubtotal.setText("");
        txtDescuencto.setText("");
        txtIva.setText("");
        txtTotal.setText("");
        txtNota.setText("");
    }
    
