    private void btnCancelarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        cerrarConexion();        
        dispose();        
    }                                           

    private void btnGuardarActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        if ( txtCantPieza.getText().equals("") ){
            JOptionPane.showMessageDialog(null, "Faltan datos por ingresar.",
                "Error", JOptionPane.ERROR_MESSAGE);
            txtCantPieza.requestFocus();
        }else{
            if( guardarOdontograma() == 1 ){
                JOptionPane.showMessageDialog(null, "Se realizo el registro correctamente",
                "Informativo", JOptionPane.INFORMATION_MESSAGE);
                cerrarConexion();
                enableBtn(true);
                btnGuardar.setEnabled(false);
            }
        }
    }                                          

    private void btnActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // Este evento afecta a todos los botones
        btngen = (JButton)evt.getSource();        
        winPiezaDental( btngen.getText() );
        btngen.setEnabled(false);
        isActivo = 0;
    }                                   

    private void btnMouseClicked(java.awt.event.MouseEvent evt) {                                 
        // Este evento afecta a todos los botones        
        btngen = (JButton)evt.getSource();
        if ( evt.getButton() == MouseEvent.BUTTON3 ) {           
            btngen.setEnabled(true);
            isActivo = 1;
        }
    }                                

    private void txtCantPiezaActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
    }                                            

    private void txtCantPiezaKeyTyped(java.awt.event.KeyEvent evt) {                                      
        int num_caracteres = 2;
        char c= evt.getKeyChar();
        if(Character.isLetter(c)){
            getToolkit().beep();
            evt.consume();    
            JOptionPane.showMessageDialog(null,"Solo se admite números" , "Validación", JOptionPane.ERROR_MESSAGE);
        }
        
        if(txtCantPieza.getText().length()== num_caracteres){ // solo admite 10 caracteres
            evt.consume();
            JOptionPane.showMessageDialog(rootPane,"Solo 2 caracteres");
        }
    }                                     

    private void txtPacienteActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           
   
    
